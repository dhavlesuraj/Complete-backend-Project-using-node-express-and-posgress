// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users{
  user_id Int @id @default(autoincrement())
  first_name String
  last_name String
  email String @unique
  password String
  mobile_no String
  gender String
  age String
  date_of_birth DateTime  
  created_at DateTime 
} 
